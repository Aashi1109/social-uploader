{
  "name": "social-uploader",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "dev:api": "tsx watch src/api/index.ts",
    "dev:workers": "tsx watch src/workers/index.ts",
    "dev:all": "run-p dev:api dev:workers",
    "start:api": "node dist/api/index.js",
    "start:workers": "node dist/workers/index.js",
    "start:worker": "node dist/workers/index.js",
    "build": "rm -rf dist && tsc -p tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "lint": "eslint .",
    "format": "prettier -w .",
    "prisma:generate": "prisma generate --schema ./src/prisma/schema.prisma",
    "migrate": "prisma migrate deploy"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^6.19.0",
    "@upstash/redis": "^1.35.6",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.13.2",
    "bullmq": "^5.63.0",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "glob": "^11.0.3",
    "googleapis": "^166.0.0",
    "ioredis": "^5.8.2",
    "pino": "^9.14.0",
    "pino-http": "^10.5.0",
    "prom-client": "^15.1.3",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@prisma/client": "^6.19.0",
    "@types/express": "^4.17.25",
    "@types/node": "^22.19.0",
    "eslint": "^9.39.1",
    "husky": "^9.1.7",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^11.3.0",
    "prettier": "^3.6.2",
    "prisma": "^6.19.0",
    "tscpaths": "^0.0.9",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
