{
  "name": "social-uploader",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "dev:api": "tsx watch src/api/index.ts",
    "dev:workers": "tsx watch src/workers/index.ts",
    "dev:all": "run-p dev:api dev:workers",
    "start:api": "node dist/api/index.js",
    "start:workers": "node dist/workers/index.js",
    "start:worker": "node dist/workers/index.js",
    "build": "rm -rf dist && tsc -p tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "lint": "eslint .",
    "format": "prettier -w ."
  },
  "dependencies": {
    "@upstash/redis": "^1.35.6",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.13.2",
    "bullmq": "^5.64.1",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "ffmpeg-static": "^5.3.0",
    "ffprobe-static": "^3.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "glob": "^13.0.0",
    "googleapis": "^166.0.0",
    "ioredis": "^5.8.2",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "prom-client": "^15.1.3",
    "sequelize": "^6.37.5",
    "sharp": "^0.34.5",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.25",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/node": "^22.19.0",
    "@types/validator": "^13.12.2",
    "eslint": "^9.39.1",
    "husky": "^9.1.7",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^11.3.0",
    "prettier": "^3.6.2",
    "tscpaths": "^0.0.9",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
